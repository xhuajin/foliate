{
    "name": "readit-obsidian-plugin",
    "version": "1.0.0",
    "description": "An Obsidian plugin built with Vite and Tailwind CSS 4",
    "main": "main.js",
    "type": "module",
    "packageManager": "pnpm@9.0.0",
    "scripts": {
        "dev": "pnpm build --watch",
        "build": "vite build && pnpm postbuild",
        "postbuild": "copy dist\\main.js . && copy dist\\styles.css . 2>nul || echo \"Build files copied\"",
        "build:watch": "vite build --watch",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "format": "prettier --write src",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "vite",
        "tailwindcss",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@headless-tree/core": "^1.4.0",
        "@headless-tree/react": "^1.4.0",
        "@radix-ui/react-icons": "^1.3.2",
        "@radix-ui/react-slot": "^1.2.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "foliate-js": "workspace:*",
        "framer-motion": "^12.23.16",
        "lucide-react": "^0.544.0",
        "obsidian-daily-notes-interface": "^0.9.4",
        "radix-ui": "^1.4.3",
        "react": "^18.3.0",
        "react-dom": "^18.3.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss-animate": "^1.0.7",
        "@zumer/snapdom": "^1.9.11"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.9",
        "@types/node": "^22.18.6",
        "@types/react": "^18.3.0",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "@vitejs/plugin-react": "^4.3.0",
        "builtin-modules": "^4.0.0",
        "eslint": "^9.0.0",
        "obsidian": "^1.7.2",
        "obsidian-typings": "latest",
        "prettier": "^3.3.0",
        "tailwind-merge": "^3.0.2",
        "tailwindcss": "^4.0.9",
        "tailwindcss-animate": "^1.0.7",
        "tslib": "^2.7.0",
        "typescript": "^5.6.0",
        "vite": "^5.4.0"
    }
}
